<%= form_for @post, html: { multipart: true } do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :price %>
  <%= f.text_field :price %>
  <%= f.label :tag_list, 'Tags' %>
  <%= f.text_field :tag_list %>
  <%= f.label :picture %>
  <%= f.file_field :picture %>
  <%= f.submit 'Create post' %>
<% end %>
